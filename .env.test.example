# Test Environment Configuration
# Copy to .env.test and fill in your values

# Required for all real API tests
FLUTTERFLOW_API_TOKEN=your_api_token_here

# Test project configuration
TEST_PROJECT_NAME=MCP-Test-Project

# Enable different test levels
REAL_API=true
MCP_INTEGRATION=true  
CLAUDE_CODE=true

# Optional: Custom API base URL
# FLUTTERFLOW_API_BASE_URL=https://api.flutterflow.io/v2

# Optional: VS Code workspace path for Claude Code tests
# VSCODE_WORKSPACE_PATH=/path/to/your/workspace

# Test timeouts (milliseconds)
TEST_TIMEOUT=60000
API_TIMEOUT=30000